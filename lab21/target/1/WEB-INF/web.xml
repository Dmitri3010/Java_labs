<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
    <servlet-name>Sss_Header</servlet-name>
    <servlet-class>Sss_Header</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>Sss_JSON</servlet-name>
    <servlet-class>Sss_JSON</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>Sss_XML</servlet-name>
    <servlet-class>Sss_XML</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Sss_Header</servlet-name>
    <url-pattern>/Sss_Header</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>Sss_XML</servlet-name>
    <url-pattern>/Sss_XML</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>Sss_JSON</servlet-name>
    <url-pattern>/Sss_JSON</url-pattern>
  </servlet-mapping>
</web-app>
